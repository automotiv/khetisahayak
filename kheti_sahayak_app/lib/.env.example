# API Configuration
# IMPORTANT: Change this to your production backend URL before building release
# For Android device testing, use your computer's local IP (e.g., http://192.168.1.100:3000/api)
# For production, use your deployed backend URL (e.g., https://api.khetisahayak.com/api)
API_BASE_URL=http://localhost:3000/api
API_TIMEOUT=30000

# Feature Flags
ENABLE_NOTIFICATIONS=true
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=false

# App Configuration
APP_NAME=Kheti Sahayak
APP_VERSION=1.0.0
