{
  "name": "kheti_sahayak_backend",
  "version": "1.0.0",
  "description": "Backend service for the Kheti Sahayak application, providing APIs for user management, marketplace, crop diagnostics, and more.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --runInBand --detectOpenHandles",
    "db:init": "node init_db.js",
    "db:seed": "node seedData.js",
    "db:seed:schemes": "node seeds/schemesSeed.js",
    "db:verify": "node scripts/verify-migrations.js",
    "migrate:create": "node-pg-migrate create",
    "migrate:up": "node-pg-migrate -r dotenv/config up",
    "migrate:down": "node-pg-migrate -r dotenv/config down",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security:audit": "node scripts/security-audit.js",
    "audit:deps": "npm audit --audit-level=moderate"
  },
  "keywords": [
    "kheti",
    "sahayak",
    "agriculture",
    "farming",
    "marketplace",
    "nodejs",
    "express"
  ],
  "author": "Your Name or Company <youremail@example.com>",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.914.0",
    "@aws-sdk/s3-request-presigner": "^3.914.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.1.0",
    "firebase-admin": "^13.6.0",
    "google-auth-library": "^9.14.0",
    "@sentry/node": "^8.0.0",
    "form-data": "^4.0.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.16",
    "pg": "^8.16.2",
    "razorpay": "^2.9.6",
    "rss-parser": "^3.13.0",
    "sharp": "^0.32.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/uuid": "^11.0.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^8.0.4",
    "nodemon": "^3.1.4",
    "pg-mem": "^2.7.0",
    "supertest": "^7.0.0",
    "uuid": "^13.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "directories": {
    "test": "tests"
  }
}